<h1>Instrumentos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
<!--      <th>Marca</th>         -->
<!--      <th>Modelo</th>        -->
<!--      <th>Unidad medida</th> -->
<!--      <th>Capacidad</th>     -->
<!--      <th>Material</th>      -->
<!--      <th>Uso</th>           -->
<!--      <th>Estado</th>        -->
      <th>Ubicacion</th>
<!--      <th>Ultima calibracion</th> -->
      <th>Responsable</th>
      <th>No bien nac</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instrumentos.each do |instrumento| %>
      <tr>
        <td><pre><%= instrumento.nombre %></pre></td>
<!--        <td><%= instrumento.marca %></td>     -->
<!--        <td><%= instrumento.modelo %></td>    -->
<!--        <td><%= instrumento.unidad_medida %></td>  -->
<!--        <td><%= instrumento.capacidad %></td>      -->
