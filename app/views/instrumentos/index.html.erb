<!--        <td><%= instrumento.uso %></td>            -->
<!--        <td><%= instrumento.estado %></td>         -->
        <td><pre><%= instrumento.ubicacion %></pre></td>
<!--        <td><%= instrumento.ultima_calibracion %></td>  -->
        <td><pre><%= instrumento.responsable %></pre></td>
        <td><pre><%= instrumento.no_bien_nac %></pre></td>
        <td><%= link_to 'Ver', instrumento %></td>
        <td><%= link_to 'Edit', edit_instrumento_path(instrumento) %></td>
        <td><%= link_to 'Elim', instrumento, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

