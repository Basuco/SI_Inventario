  <tbody>
    <% @equipos.each do |equipo| %>
      <tr>
        <td><pre><%= equipo.nombre %></pre></td>
<!--         <td><%= equipo.marca %></td> -->
<!--         <td><%= equipo.modelo %></td> -->
<!--         <td><%= equipo.serial %></td> -->
        <td><pre><%= equipo.no_bien_nac %></pre></td>
